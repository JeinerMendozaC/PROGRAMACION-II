#16.10 EJERCICIO 10
print("L=[1,2,3,4,5,6,7,8,9]")
print("L1=[10,11,12,13,14,15]")
A=input("QUE LISTA DESEA VACIAR:")
A=A.upper()
L=[1,2,3,4,5,6,7,8,9]
L1=[10,11,12,13,14,15]
if A=="L":
    L.clear()
    print(L)
    print(L1)
elif A=="L1":
    L1.clear()
    print(L1)
    print(L)
else:
    print("INGRESE DATOS CORRECTOS ")

