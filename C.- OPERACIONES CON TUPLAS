if 1230 not  in A:
    print("TRUE")

#2.7 EJERCICIO 7
QW="A",2,"B",3,"C"
if "K" not in QW:
    print("TRUE")

#2.8 EJERCICIO 8
N=int(input("ingrese un numero:"))
