print("##  CONJUNTOS ##")
ELEMENT1=set(input("INGRESE UNA SERIE DE ELEMENTOS [CONJUNTO 1]:"))
ELEMENT2=set(input("INGRESE OTRA SERIE DE ELEMENTOS[CONJUNTO 2]:"))
print("####################################")
print(ELEMENT1)
print(ELEMENT2)
MA=max(ELEMENT1)
MI=min(ELEMENT1)
MA2=max(ELEMENT2)
MI2=min(ELEMENT2)
print("####################################")
print("EL MAXIMO ELEMENTO DEL CONJUNTO 1 ES:",MA)
print("EL MINIMO ELEMENTO DEL CONJUNTO 1 ES:",MI)
print("EL MAXIMO ELEMENTO DEL CONJUNTO 2 ES:",MA2)
print("EL MINIMO ELEMENTO DEL CONJUNTO 2 ES:",MI2)
print("####################################")
print("####  UNION   ####")
X=ELEMENT1.union(ELEMENT2 )
print("####################################")
print("LA UNION DE LOS CONJUNTOS ES:",X)
print("####################################")
print("###   DIFERENCIA   ####")
Y=ELEMENT1-ELEMENT2
C=ELEMENT2-ELEMENT1
print("####################################")
print("LOS ELEMENTOS DEL [COMNJUNTO 1] QUE NO ESTAN EN EL [CONJUNTO 2] SON:",Y)
print("LOS ELEMENTOS DEL [COMNJUNTO 2] QUE NO ESTAN EN EL [CONJUNTO 1] SON:",C)
print("####################################")
print("### INTERSECCION  ###")
IN=ELEMENT1&ELEMENT2
print("LOS ELEMENTOS EN COMUN DE LOS CONJUNTOS SON:",IN)
print("####################################")
print("")
print("####################################")
print("###  EL PROGRAMA A FINALIZADO   ####")
print("####################################")

